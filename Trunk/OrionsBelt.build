				commandline="--locales:'en;pt;es;hr;fr;de' --localize:../Localization"/>

    <copy file="WebUserInterface/Localization/Script/I18N-pt.js" todir="WebResources/Scripts/" overwrite="true" />
    <copy file="WebUserInterface/Localization/Script/I18N-en.js" todir="WebResources/Scripts/" overwrite="true" />
    <copy file="WebUserInterface/Localization/Script/I18N-es.js" todir="WebResources/Scripts/" overwrite="true" />
