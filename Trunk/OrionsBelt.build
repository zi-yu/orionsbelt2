  <target name="deploy" description="Deploys OrionsBelt" depends="build-all clean-deploy">
    <call target="scripts" failonerror="false"/>
    <call target="deploy-web" failonerror="false"/>
    <call target="deploy-resources" failonerror="false"/>
    <call target="deploy-manual" failonerror="false"/>
    <call target="deploy-unittests" failonerror="false"/>
    <call target="deploy-bots" />
    <call target="deploy-botHandler" />
  </target>

  <target name="patch" description="Deploys OrionsBelt" >
    <call target="deploy-web" />
    <delete dir="Deploy/WebPreCompiled/Admin"/>
    <delete>
      <fileset>
        <include name="Deploy/WebPrecompiled/Bin/*.dll" />
        <exclude name="Deploy/WebPrecompiled/Bin/Orionsbelt*.dll" />
      </fileset>
    </delete>

    <delete>
      <fileset>
        <include name="Deploy/WebPrecompiled/Jobs/*.dll" />
        <exclude name="Deploy/WebPrecompiled/Jobs/Orionsbelt*.dll" />
      </fileset>
    </delete>
  </target>

  <target name="scripts" description="Prepares scripts" depends="localize">

    <exec workingdir="WebUserInterface/Bin"
				basedir="WebUserInterface/Bin"
				program="OrionsBelt.Chronos.exe"
				useruntimeengine="true"
				commandline="--scripts-merge:../../WebResources"/>

    <echo message="Running YUI Compressor for Javascript..." />
    <exec program="java"
				commandline="-jar ${yuicompressor} WebResources/Scripts/Temp/EngineStep1.temp.js --charset utf8 --preserve-semi --type js -o WebResources/Scripts/Temp/EngineStep2.temp.js "/>

    <echo message="Running YUI Compressor for CSS..." />
    <exec program="java"
				commandline="-jar ${yuicompressor} WebResources/Styles/Temp/EngineStep1.temp.css --charset utf8 --preserve-semi --type css -o WebResources/Styles/Temp/EngineStep2.temp.css "/>

    <xmlpeek
      file="WebUserInterface/Web.config"
      xpath="/configuration/appSettings/add[@key = 'scriptVersion']/@value"
      property="ScriptsVersion">
    </xmlpeek>

    <echo message="Scripts Version: ${ScriptsVersion}" />

    <mkdir dir="WebResources/Scripts/v${ScriptsVersion}/" />
    <copy file="WebResources/Scripts/Temp/EngineStep2.temp.js" tofile="WebResources/Scripts/v${ScriptsVersion}/Engine.js" overwrite="true" />
    <mkdir dir="WebResources/Scripts/v/" />
    <copy file="WebResources/Scripts/Temp/EngineStep2.temp.js" tofile="WebResources/Scripts/v/Engine.js" overwrite="true" />
    <mkdir dir="WebResources/Styles/v${ScriptsVersion}/" />
    <copy file="WebResources/Styles/Temp/EngineStep2.temp.css" tofile="WebResources/Styles/v${ScriptsVersion}/Engine.css" overwrite="true" />
    <mkdir dir="WebResources/Styles/v/" />
    <copy file="WebResources/Styles/Temp/EngineStep2.temp.css" tofile="WebResources/Styles/v/Engine.css" overwrite="true" />

    <exec workingdir="WebUserInterface/Bin"
				basedir="WebUserInterface/Bin"
				program="OrionsBelt.Chronos.exe"
				commandline="--scripts-I18N:../../WebResources/Scripts/v${ScriptsVersion}/Engine.js"/>

    <exec workingdir="WebUserInterface/Bin"
        basedir="WebUserInterface/Bin"
        program="OrionsBelt.Config.exe"
	useruntimeengine="true"
        commandline="-gzip-scripts:../../WebResources/Scripts/v${ScriptsVersion}/"/>

    <exec workingdir="WebUserInterface/Bin"
        basedir="WebUserInterface/Bin"
        program="OrionsBelt.Config.exe"
	useruntimeengine="true"
        commandline="-gzip-scripts:../../WebResources/Styles/v${ScriptsVersion}/"/>

  </target>

