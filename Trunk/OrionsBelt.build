    <copy todir="Deploy/WebPrecompiled/Admin" overwrite="true">
      <fileset basedir="WebUserInterface/Admin">
        <include name="Bin/*.dll" />
        <include name="Bin/*.exe" />
        <include name="**/*.aspx" />
